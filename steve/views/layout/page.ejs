    <div class="container-fluid">
      <a href="#" class="navbar-brand">
        <img src="../images/logo.png" height="35" alt="CoolBrand" />
      </a>
      <button type="button" class="navbar-toggler" data-bs-toggle="collapse" data-bs-target="#navbarCollapse9">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarCollapse9">
        <div class="navbar-nav">
          <a href="/" class="nav-item nav-link active">маинкрафт</a>
          <% if(typeof nav=='object' && nav){ nav.forEach(function(steve){ %>
            <a class="nav-item nav-link" href="/steve/<%=steve.nick%>">
              <%=steve.title%>
            </a>
            <% })} %>
          <a href="/vizer" class="nav-item nav-link">визер</a>
          <a href="/steve" class="nav-item nav-link">стив</a>
          <a href="/dragon" class="nav-item nav-link">дракон</a>
          <a href="/maincraft" class="nav-item nav-link">маинкрафт</a>
          <a href="/IIIytel" class="nav-item nav-link">житель</a>
          <a href="/skelet" class="nav-item nav-link">скелет</a> 
          <a href="/zomby" class="nav-item nav-link">зомби</a> 
        </div>
        <div class="navbar-nav ms-auto">
          <a href="#" class="nav-item nav-link">Login</a>
        </div>
      </div>
    </div>
  </nav>
  <%- body %>
</body>

</html>